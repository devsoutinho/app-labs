{
    "version": "1.0.0",
    "license": "MIT",
    "type": "module",
    "name": "@devsoutinho/webproxy",
    "scripts": {
        "setup-clojure-deps": "rm -rf nbb-deps && bb write-deps",
        "predev": "npx kill-port 3000",
        "dev": "PORT=3000 nodemon --ext 'js,cljs' --exec 'yarn start'",
        "start": "nbb ./webproxy/app/server.cljs",
        "repl": "nbb nrepl-server :port 1337",
        "deploy": "git push heroku main"
    },
    "dependencies": {
        "@tinyhttp/app": "^2.0.19",
        "@tinyhttp/logger": "^1.3.0",
        "milliparsec": "^2.2.2",
        "nbb": "0.5.104"
    },
    "devDependencies": {
        "nodemon": "2.0.16"
    },
    "engines": {
        "yarn": "1.x"
    }
}
