{
    "name": "devsoutinho-app-labs",
    "version": "0.0.0",
    "private": true,
    "workspaces": {
      "packages": [
        "_servers/*",
        "_apps/*"
      ]
    },
    "scripts": {
      "devsoutinho_webproxy": "yarn workspace @devsoutinho/webproxy",
      "start": "yarn devsoutinho_webproxy start",
      "repl": "yarn devsoutinho_webproxy repl",
      "devsoutinho_app": "yarn workspace devsoutinho_app",
      "dev:web": "yarn devsoutinho_app dev:web",
      "dev:mac": "yarn devsoutinho_app dev:mac",
      "dev:win": "yarn devsoutinho_app dev:win",
      "dev:linux": "yarn devsoutinho_app dev:linux",
      "build:web": "yarn devsoutinho_app build:web",
      "build:web:dev": "yarn devsoutinho_app build:web:dev",
      "build:ios": "yarn devsoutinho_app build:ios",
      "deploy:web": "yarn devsoutinho_app deploy:web",
      "test:unit": "yarn devsoutinho_app test:unit",
      "deploy:web:preview": "yarn devsoutinho_app deploy:web:preview",
      "generate:icons": "yarn devsoutinho_app generate:icons",
      "generate:app_name": "yarn devsoutinho_app generate:app_name"
    },
    "main": "index.js",
    "author": "Mario Souto <13791385+omariosouto@users.noreply.github.com>",
    "license": "MIT"
  }